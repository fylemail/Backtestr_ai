# Development Environment Configuration
NODE_ENV=development
RUST_LOG=debug

# Database Configuration
DB_PATH=./data/development.duckdb
DB_MAX_MEMORY=4GB
DB_THREADS=4

# Engine Configuration
ENGINE_TICK_BUFFER_SIZE=100000
ENGINE_MAX_PARALLEL_ALGORITHMS=4
ENGINE_PYTHON_THREADS=2

# IPC Configuration
IPC_PORT=7878
IPC_MAX_MESSAGE_SIZE=10485760

# API Configuration
UPDATE_SERVER_URL=http://localhost:8080
TELEMETRY_ENABLED=false

# Development Features
HOT_RELOAD=true
DEBUG_MODE=true
PROFILING_ENABLED=true

# Paths
ALGORITHM_PATH=./algorithms
DATA_PATH=./data
CACHE_PATH=./data/cache